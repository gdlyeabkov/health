<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="SpeakableTextPresentCheck">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:background="#F0F0F0"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/main_page_active_block"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/main_page_active_block_aside"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/main_page_active_block_aside_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Активность"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:id="@+id/main_page_active_block_aside_footer"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/main_page_active_block_aside_footer_walk"
                        android:layout_width="25dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/main_page_active_block_aside_footer_walk_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            app:srcCompat="@drawable/steps_logo"
                            tools:srcCompat="@drawable/steps_logo" />

                        <TextView
                            android:id="@+id/main_page_active_block_aside_footer_walk_label"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="55"
                            android:textAlignment="center"
                            android:textSize="18sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_page_active_block_aside_footer_sleep"
                        android:layout_width="25dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/main_page_active_block_aside_footer_sleep_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            app:srcCompat="@drawable/time_activity_logo"
                            tools:srcCompat="@drawable/time_activity_logo" />

                        <TextView
                            android:id="@+id/main_page_active_block_aside_footer_sleep_label"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textAlignment="center"
                            android:textSize="18sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_page_active_block_aside_footer_callories"
                        android:layout_width="25dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/main_page_active_block_aside_footer_callories_icon"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            app:srcCompat="@drawable/active_calories_logo"
                            tools:srcCompat="@drawable/active_calories_logo" />

                        <TextView
                            android:id="@+id/main_page_active_block_aside_footer_callories_label"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="2"
                            android:textAlignment="center"
                            android:textSize="18sp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/main_page_active_block_icon"
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                app:srcCompat="@drawable/activity_logo"
                tools:srcCompat="@drawable/activity_logo" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_walk_block"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="#FFFFFF"
            android:orientation="vertical"
            android:outlineProvider="bounds">

            <ImageButton
                android:id="@+id/imageButton27"
                android:layout_width="40dp"
                android:layout_height="35dp"
                android:layout_marginLeft="350dp"
                android:layout_weight="1"
                android:scaleType="fitStart"
                android:visibility="gone"
                app:srcCompat="@drawable/minus_logo" />

            <TextView
                android:id="@+id/main_page_walk_block_header"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="Шаги"
                android:textSize="18sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/main_page_walk_block_data"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginLeft="25dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/main_page_walk_block_data_aside"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/main_page_walk_block_data_aside_value"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="55"
                        android:textAlignment="center"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        tools:text="55" />

                    <TextView
                        android:id="@+id/main_page_walk_block_data_aside_max_value"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="75dp"
                        android:layout_weight="1"
                        android:text="/6000" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/main_page_walk_block_data_article"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:textAlignment="center">

                    <TextView
                        android:id="@+id/main_page_walk_block_data_aritcle_progress_label"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_weight="1"
                        android:text="0%" />

                    <ProgressBar
                        android:id="@+id/main_page_walk_block_data_aritcle_progress"
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_exercise_block"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="#FFFFFF"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/main_page_exercise_block_header"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="15dp"
                android:orientation="horizontal"
                android:visibility="visible">

                <TextView
                    android:id="@+id/main_page_exercise_block_header_label"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_weight="1"
                    android:text="Упражнение"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/main_page_exercise_block_header_logs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1"
                    android:text="Просмотреть журнал"
                    android:textAlignment="textEnd"
                    android:textColor="#A5A5A5"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/main_page_exercise_block_exercises"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/main_page_exercise_block_exercises_walk"
                    android:layout_width="150dp"
                    android:layout_height="55dp"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:backgroundTint="#FFFFFF"
                    android:scaleType="fitXY"
                    android:src="@drawable/walk_logo"
                    android:visibility="gone"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageButton
                    android:id="@+id/main_page_exercise_block_exercises_run"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:backgroundTint="#FFFFFF"
                    android:scaleType="fitXY"
                    android:src="@drawable/run_logo"
                    android:visibility="gone"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/main_page_exercise_block_exercises_bicycle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:backgroundTint="#FFFFFF"
                    android:scaleType="fitXY"
                    android:src="@drawable/bicycle_logo"
                    android:visibility="gone"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageButton
                    android:id="@+id/main_page_exercise_block_exercises_list"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="35dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:backgroundTint="#FFFFFF"
                    android:scaleType="fitXY"
                    android:src="@drawable/list_logo"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_exercise_block_activated"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:background="#00AF00"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/main_page_exercise_block_activated_header"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="15dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/main_page_exercise_block_activated_header_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_weight="1"
                    android:text="Ходьба"
                    android:textColor="#FFFFFF" />

                <TextView
                    android:id="@+id/main_page_exercise_block_activated_header_start_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="15dp"
                    android:layout_weight="1"
                    android:text="Начало 10:43"
                    android:textAlignment="textEnd"
                    android:textColor="#FFFFFF" />
            </LinearLayout>

            <TextView
                android:id="@+id/main_page_exercise_block_activated_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="00:00:00"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="24sp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_food_block"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="#FFFFFF"
            android:orientation="vertical">

            <TextView
                android:id="@+id/main_page_food_block_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="15dp"
                android:text="Еда"
                android:textStyle="bold" />

            <Button
                android:id="@+id/main_page_food_block_add_record_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginRight="50dp"
                android:backgroundTint="#FFFFFF"
                android:text="Запись"
                android:textColor="#000000"
                android:textSize="12sp"
                app:cornerRadius="100dp" />

            <LinearLayout
                android:id="@+id/main_page_food_block_body"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="25dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/main_page_food_block_current_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="0"
                    android:textAlignment="center"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/main_page_food_max_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="250dp"
                    android:layout_weight="1"
                    android:text="/1655 ккал" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_sleep_block"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="#FFFFFF"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="15dp"
                android:text="Сон"
                android:textStyle="bold" />

            <Button
                android:id="@+id/main_page_sleep_block_add_record_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="250dp"
                android:layout_marginRight="50dp"
                android:backgroundTint="#FFFFFF"
                android:text="Запись"
                android:textColor="#000000"
                android:textSize="12sp"
                app:cornerRadius="100dp" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:text="Как вам спалось?" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_body_composition_block"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="#FFFFFF"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/main_page_body_composition_block_wrap"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/main_page_body_composition_block_wrap_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:layout_marginTop="15dp"
                    android:text="Состав тела"
                    android:textStyle="bold" />

                <LinearLayout
                    android:id="@+id/main_page_body_composition_block_wrap_body"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="25dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/main_page_body_composition_block_wrap_body_icon"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:translationX="50dp"
                        app:srcCompat="@drawable/body_composition_logo"
                        tools:srcCompat="@drawable/body_composition_logo" />

                    <ImageView
                        android:id="@+id/main_page_body_composition_block_wrap_body_icon_fat"
                        android:layout_width="55dp"
                        android:layout_height="25dp"
                        android:layout_weight="1"
                        android:scaleType="fitXY"
                        android:translationX="100dp"
                        android:visibility="gone"
                        app:srcCompat="@drawable/fat_logo" />

                    <ImageView
                        android:id="@+id/main_page_body_composition_block_wrap_body_icon_musculature"
                        android:layout_width="55dp"
                        android:layout_height="25dp"
                        android:layout_weight="1"
                        android:scaleType="fitXY"
                        android:translationX="150dp"
                        android:visibility="gone"
                        app:srcCompat="@drawable/musculature_logo" />

                    <Button
                        android:id="@+id/main_page_body_composition_block_wrap_body_add_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="200dp"
                        android:layout_marginTop="0dp"
                        android:layout_marginRight="55dp"
                        android:layout_marginBottom="0dp"
                        android:layout_weight="1"
                        android:backgroundTint="#FFFFFF"
                        android:foregroundTint="#FFFFFF"
                        android:text="Запись"
                        android:textColor="#000000"
                        android:textSize="10sp"
                        app:cornerRadius="100dp"
                        tools:ignore="TouchTargetSizeCheck" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/main_page_body_composition_block_wrap_footer"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="25dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/main_page_body_composition_block_wrap_footer_weight"
                        android:layout_width="10dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_weight_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_weight="1"
                            android:text="--"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_weight_measure"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="кг."
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_page_body_composition_block_wrap_footer_fat"
                        android:layout_width="10dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_fat_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="TextView" />

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_fat_measure"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="%"
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_page_body_composition_block_wrap_footer_musculature"
                        android:layout_width="10dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_musculature_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="TextView" />

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_musculature_measure"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="кг"
                            android:visibility="gone" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/main_page_body_composition_block_wrap_footer_data"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/main_page_body_composition_block_wrap_footer_data_weight"
                        android:layout_width="35dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:textAlignment="center">

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_data_weight_label"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="TextView"
                            android:textSize="20sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_page_body_composition_block_wrap_footer_data_fat"
                        android:layout_width="35dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:textAlignment="center">

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_data_fat_label"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="TextView"
                            android:textSize="20sp"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_page_body_composition_block_wrap_footer_data_musculature"
                        android:layout_width="35dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:textAlignment="center">

                        <TextView
                            android:id="@+id/main_page_body_composition_block_wrap_footer_data_musculature_label"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="TextView"
                            android:textSize="20sp"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/main_page_water_block"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="100dp"
            android:background="#FFFFFF"
            android:orientation="vertical">

            <TextView
                android:id="@+id/main_page_water_block_header"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_marginLeft="35dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:text="Вода"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/main_page_water_block_drink_glasses"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_weight="1"
                    android:text="0"
                    android:textAlignment="center"
                    android:textSize="24sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/main_page_water_block_drink_glasses_label"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="0dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:text="стак." />

                <Button
                    android:id="@+id/main_page_water_block_drink_glasses_decrease_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50dp"
                    android:layout_marginRight="0dp"
                    android:layout_weight="1"
                    android:backgroundTint="#FFFFFF"
                    android:enabled="false"
                    android:outlineProvider="none"
                    android:text="-"
                    android:textColor="#969696"
                    android:textStyle="bold"
                    app:cornerRadius="500dp"
                    app:strokeColor="#000000"
                    app:strokeWidth="1sp"
                    tools:ignore="TouchTargetSizeCheck" />

                <Button
                    android:id="@+id/main_page_water_block_drink_glasses_increase_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="110dp"
                    android:layout_weight="1"
